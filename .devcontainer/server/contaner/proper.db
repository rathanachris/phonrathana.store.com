#  CPU in power saving 
ro.config.performance_mode=true

#  background task Success CPU
ro.sys.fw.bg_apps_limit=0

# Boost Scheduler
sys.scheduler.boost=1
persist.sys.NV_CPU_BOOST=1

# Binder performance
persist.sys.purgeable_assets=1

# Touch latency reduction
touch.pressure.scale=0.001
touch.size.scale=0.001

# GPU & UI boost
persist.sys.ui.hw=1
ro.hwui.render_dirty_regions=false
